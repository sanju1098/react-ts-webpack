{
	"name": "react-ts-webpack",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "cross-env NODE_ENV=development webpack serve --config webpack/webpack.config.js --env env=dev --open",
		"build:qa": "cross-env NODE_ENV=qa webpack --config webpack/webpack.config.js --env env=qa",
		"start:qa": "cross-env NODE_ENV=qa webpack --config webpack/webpack.config.js --env env=qa && serve dist_qa/ --single",
		"build:stg": "cross-env NODE_ENV=stg webpack --config webpack/webpack.config.js --env env=stg",
		"start:stg": "cross-env NODE_ENV=stg webpack --config webpack/webpack.config.js --env env=stg  && serve dist_stg/ --single",
		"build:production": "cross-env NODE_ENV=prod webpack --config webpack/webpack.config.js --env env=prod",
		"start:production": "cross-env NODE_ENV=prod webpack --config webpack/webpack.config.js --env env=prod && serve dist_prod/ --single"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
